@using IdentityServer_FrontEnd.ViewModels;

@model MainPageViewModel

<head>
    <link href=@Url.Content("~/styles/app.css") rel="stylesheet" type="text/css" />
</head>
<form asp-controller="Pages" asp-action="MainPage" metod="get">
    <div class="heared">Hello @Model.UserName</div>
    <div class="block">
        <button type="submit" class="button">
            <a asp-controller="Auth" asp-action="LogOut" asp-route-logOutId="@Model.UserName">
                Log Out
            </a>
        </button>
    </div>
</form>